{
  "name": "universal-react-query",
  "version": "0.1.0",
  "description": "Create Universal Library for @tanstack/react-query v4 and 5",
  "keywords": [
    "@tanstack/react-query",
    "react-query",
    "universal-react-query"
  ],
  "homepage": "https://github.com/gwansikk/universal-react-query",
  "bugs": "https://github.com/gwansikk/universal-react-query/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gwansikk/universal-react-query.git"
  },
  "license": "MIT",
  "author": "Gwansik Kim <gwansik.kim@gwansik.dev>",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "bin",
    "dist",
    "scripts"
  ],
  "scripts": {
    "postinstall": "node -e \"import('./scripts/postinstall.mjs').catch(e => console.error(e))\"",
    "build": "tsup"
  },
  "bin": {
    "universal-react-query-fix": "bin/universal-react-query-fix.mjs",
    "universal-react-query-switch": "bin/universal-react-query-switch.mjs"
  },
  "devDependencies": {
    "@types/node": "^20.14.6",
    "prettier": "^3.3.2",
    "tsup": "^8.1.0",
    "typescript": "^5.4.5"
  },
  "peerDependencies": {
    "@tanstack/react-query": "^4 || ^5"
  },
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "pnpm@9.4.0"
}
